<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
      <servlet-name>ServletForward</servlet-name>
      <servlet-class>ServletForward</servlet-class>
    </servlet>
    <servlet>
    <servlet-name>ServletForwardEX</servlet-name>
    <servlet-class>ServletForwardExample</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>ServletRedirect</servlet-name>
      <servlet-class>ServletRedirect</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>ServletRedirectExample</servlet-name>
      <servlet-class>ServletRedirectExample</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>ServletForward</servlet-name>
      <url-pattern>/forward</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
      <servlet-name>ServletForwardEX</servlet-name>
      <url-pattern>/servletFE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
      <servlet-name>ServletRedirect</servlet-name>
      <url-pattern>/servletredirect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
      <servlet-name>ServletRedirectExample</servlet-name>
      <url-pattern>/servletRE</url-pattern>
    </servlet-mapping>
</web-app>
